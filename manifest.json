
{
  "short_name": "Tabs Restore",
  "name": "Restores saved Tabs",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "http://*/*", 
    "https://*/*"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "/imgs/icon-16.png",
      "32": "/imgs/icon-32.png",
      "48": "/imgs/icon-48.png",
      "128": "/imgs/icon-128.png" 
    }
  },
  "icons": {
    "16": "/imgs/icon-16.png",
    "32": "/imgs/icon-32.png",
    "48": "/imgs/icon-48.png",
    "128": "/imgs/icon-128.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-xO+Ik1PZ+p+tCQpUbVrktJQUYdvbfPXDHQC+AaZVzRE='; object-src 'self';"
}